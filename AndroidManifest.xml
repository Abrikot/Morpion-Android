<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sebere_peree_dulie_cornaton.xoxo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/white_token"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuActivity"
            android:parentActivityName=".ConnectionActivity" />
        <activity
            android:name=".BoardActivity"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".ConnectionActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".RulesActivity" />
        <activity
            android:name=".EndGameActivity"
            android:parentActivityName=".MenuActivity" />
        <activity android:name=".ScoresActivity" />
        <activity android:name=".HistoryActivity" />
    </application>

</manifest>